{
  "extends": [
    "openlayers",
    "eslint-config-airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "quote-props": ["error", "as-needed"],
    "no-multi-spaces": "error",
    "object-shorthand": ["error", "always"],
    "prefer-template": "error",
    "prefer-const": "error"
  }
}
