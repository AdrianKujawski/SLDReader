{
  "name": "sldreader",
  "version": "0.0.0",
  "description": "SLD reader and formatter for openlayers",
  "main": "dist/bundle.js",
  "scripts": {
    "prepublish": "npm run doc && npm run build",
    "test": "./node_modules/karma/bin/karma start karma.conf.js",
    "build": "gulp build",
    "docs":
      "documentation build src/index.js -c docs/documentation.yml -f md --output docs/api.md && cp dist/bundle.js docs/examples/"
  },
  "bugs": {
    "url": "https://github.com/NieuwlandGeo/SLDReader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NieuwlandGeo/SLDReader"
  },
  "author": {
    "name": "Allart Kooiman",
    "email": "akooiman@nieuwland.nl"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-merge-imports": "github:ghettovoice/babel-plugin-merge-imports",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "chai": "^4.0.2",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-openlayers": "^7.0.0",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "mocha": "^3.4.2",
    "ol": "^4.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {},
  "browserify-shim": {
    "openlayers": "global:ol"
  }
}
