{
  "name": "sldreader",
  "version": "0.0.0",
  "description": "SLD reader and formatter for openlayers",
  "main": "dist/bundle.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "browser-sync start --config bs-config.js",
    "start": "npm-run-all -p serve watch",
    "docs": "documentation build src/index.js -c docs/documentation.yml -f md --output docs/api.md"
  },
  "bugs": {
    "url": "https://github.com/NieuwlandGeo/SLDReader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NieuwlandGeo/SLDReader"
  },
  "author": {
    "name": "Allart Kooiman",
    "email": "akooiman@nieuwland.nl"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.23.6",
    "chai": "^4.1.2",
    "documentation": "^6.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-openlayers": "^7.0.0",
    "eslint-plugin-import": "^2.7.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.2",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "ol": "^4.2.0"
  },
  "browserify-shim": {
    "openlayers": "global:ol"
  }
}
